{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "~~/*": ["./*"],
      "@@/*": ["./*"],
      "~/*": ["./src/*"],
      "@/*": ["./src/*"],
      "@components/*": ["./src/components/*"],
      "@partials/*": ["./src/partials/*"],
      "@lib/*": ["./src/lib/*"],
      "@utils/*": ["./src/utils/*"],
      "@mixins/*": ["./src/mixins/*"]
    }
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}
